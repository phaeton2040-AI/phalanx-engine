# Server port
PORT=3000

# Environment mode (development | production)
NODE_ENV=development

# CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3001

# Google OAuth Client ID (required for authentication)
# Get this from Google Cloud Console: https://console.cloud.google.com/apis/credentials
# Must match the client ID used in the babylon-ecs game client
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com

# Google OAuth Client Secret (required for token exchange)
# Get this from Google Cloud Console along with the Client ID
# IMPORTANT: Keep this secret! Never expose it in client-side code.
GOOGLE_CLIENT_SECRET=your-client-secret

