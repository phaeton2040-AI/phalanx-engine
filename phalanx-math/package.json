{
  "name": "phalanx-math",
  "version": "0.1.0",
  "description": "Deterministic fixed-point math library for Phalanx Engine - ensures identical calculations across all platforms",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fixed-point",
    "deterministic",
    "math",
    "game-math",
    "cross-platform",
    "phalanx"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@hastom/fixed-point": "^4.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}

